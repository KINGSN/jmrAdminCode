<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        app:cardBackgroundColor="#F3F3F3"
        app:cardCornerRadius="10dp"
        app:cardElevation="0dp">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="horizontal">

        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:cardCornerRadius="10dp"
            app:cardElevation="0dp">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/idImage"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/round_bg" />


        </androidx.cardview.widget.CardView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="14dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/tvCreatedIdTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/plusjakarta_text_regular"
                    android:maxLines="1"
                    android:textSize="15sp"
                    android:text="special tech"
                    android:textColor="#2e2e2e"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tvCreatedIdUserStatus"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:background="#CCFFC9"
                    android:fontFamily="@font/plusjakarta_text_regular"
                    android:maxLines="1"
                    android:padding="3dp"
                    android:text="Enabled"
                    android:textColor="#2e2e2e"
                    android:textSize="12sp"
                    android:textStyle="normal" />

            </RelativeLayout>

            <TextView
                android:id="@+id/tvCreatedIdWebsite"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:fontFamily="@font/plusjakarta_text_regular"
                android:maxLines="1"
                android:text="Specialtask.com"
                android:textColor="#2e2e2e"
                android:textSize="12sp"
                android:textStyle="normal" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/plusjakarta_text_regular"
                    android:maxLines="1"
                    android:text="Order ID  : "
                    android:textColor="#2e2e2e"
                    android:textSize="12sp"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/orderId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/plusjakarta_text_regular"
                    android:maxLines="1"
                    android:text="#chd1234"
                    android:textColor="#2e2e2e"
                    android:textSize="12sp"
                    android:textStyle="normal" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/plusjakarta_text_regular"
                    android:maxLines="1"
                    android:text="Registered No : "
                    android:textColor="#2e2e2e"
                    android:textSize="12sp"
                    android:textStyle="normal" />


                <TextView
                    android:id="@+id/tvMobile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/plusjakarta_text_regular"
                    android:maxLines="1"
                    android:text="8251941213"
                    android:textColor="#2e2e2e"
                    android:textSize="12sp"
                    android:textStyle="normal" />

            </LinearLayout>
            <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:fontFamily="@font/plusjakarta_text_regular"
                android:maxLines="1"
                android:text="Id UserName : "
                android:textColor="#2e2e2e"
                android:textSize="12sp"
                android:textStyle="normal" />

            <TextView
                android:id="@+id/appUserName"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center_vertical"
                android:fontFamily="@font/plusjakarta_text_regular"
                android:maxLines="1"
                android:text="admin"
                android:textColor="#2e2e2e"
                android:textSize="12sp"
                android:textStyle="normal" />


        </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/plusjakarta_text_regular"
                    android:maxLines="1"
                    android:text="Id Password : "
                    android:textColor="#2e2e2e"
                    android:textSize="12sp"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/idPassword"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/plusjakarta_text_regular"
                    android:maxLines="1"
                    android:text="admin"
                    android:textColor="#2e2e2e"
                    android:textSize="12sp"
                    android:textStyle="normal" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/plusjakarta_text_regular"
                    android:maxLines="1"
                    android:text="Request Date : "
                    android:textColor="#2e2e2e"
                    android:textSize="12sp"
                    android:textStyle="normal" />


                <TextView
                    android:id="@+id/tvCreatedIdTimestamp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/plusjakarta_text_regular"
                    android:maxLines="2"
                    android:text="02-12-2000 | Midnight"
                    android:textColor="@color/colorPrimary"
                    android:textSize="12sp"
                    android:textStyle="normal" />




            </LinearLayout>

            <LinearLayout
                android:id="@+id/lverify"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp">

                <TextView
                    android:id="@+id/vorcan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/plusjakarta_text_regular"
                    android:maxLines="1"
                    android:text="Verified Date : "
                    android:textColor="#2e2e2e"
                    android:textSize="12sp"
                    android:textStyle="normal" />


                <TextView
                    android:id="@+id/tvVerifiedTimestamp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/plusjakarta_text_regular"
                    android:maxLines="2"
                    android:text="02-12-2000 | Midnight"
                    android:textColor="@color/colorPrimary"
                    android:textSize="12sp"
                    android:textStyle="normal" />




            </LinearLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">



                <LinearLayout
                    android:id="@+id/actn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:orientation="horizontal"
                    tools:ignore="UselessParent">

                    <ImageView
                        android:id="@+id/btnEditCreatedId"
                        android:layout_width="30dp"
                        android:clickable="true"
                        android:layout_height="30dp"
                        android:src="@drawable/ic_edit"
                        app:tint="#0A8313"
                        android:focusable="true"
                        android:contentDescription="TODO" />

                    <ImageView
                        android:id="@+id/btnDeleteCreatedId"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:clickable="true"
                        android:layout_marginStart="10dp"
                        android:src="@drawable/ic_cancelled"
                        app:tint="#B71C1C"
                        android:focusable="true"
                        android:contentDescription="TODO" />


                </LinearLayout>

            </RelativeLayout>


        </LinearLayout>


    </LinearLayout>


</androidx.cardview.widget.CardView>
    </layout>